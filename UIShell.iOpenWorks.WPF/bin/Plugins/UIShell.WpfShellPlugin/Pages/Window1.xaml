<Window x:Class="UIShell.WpfShellPlugin.Pages.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="数量确认" Height="300" Width="300">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="0,100,10,0">

            <TextBlock Name="textBlock_TakeNum" Text="请输入提取数量:" Height="24" VerticalAlignment="Top" HorizontalAlignment="Left" />
            <TextBox x:Name="textBox_TakeNum" MinWidth="50" VerticalAlignment="Top"  HorizontalAlignment="Left" Height="24" Width="51"  />
            <Button HorizontalAlignment="Left"  Click="button1_Click"  x:Name="button2" Height="24" VerticalAlignment="Top" Content="确认" Width="63" Margin="10,0,0,0"/>

        </StackPanel>
    </Grid>
</Window>
